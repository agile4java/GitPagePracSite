{{#if success_msg}}
  <div class="alert alert-success alert-dismissable fade show">
    <button type="button" class="close" data-dismiss="alert">
      &times;
    </button>
    {{success_msg}} </div>
{{/if}}

{{#if error_msg}}
  <div class="alert alert-danger alert-dismissable">{{error_msg}}
    &times;
  </div>
{{/if}}

